set nocompatible
filetype off 
set rtp+=~/.vim/bundle/Vundle.vim


call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
Plugin 'tomlion/vim-solidity' 
Plugin 'othreaa/yajs.vim'
